{
  "name": "icall-cli",
  "version": "0.0.16",
  "description": "A cli when you think",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "bin": {
    "icall": "src/bin/call.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "dev:production": "node dist/index.js",
    "build": "unbuild",
    "commit": "git-cz",
    "changeset": "changeset",
    "version": "changeset version",
    "prepare": "husky install",
    "publish": "npm publish --registry=https://registry.npmjs.org/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "icall",
    "cli",
    "windows",
    "linux",
    "macOS",
    "cat",
    "cd",
    "cp",
    "day",
    "exec",
    "ls",
    "mkdir",
    "mv",
    "mkdir",
    "ping",
    "rm",
    "top",
    "touch",
    "tree",
    "which",
    "whoami",
    "x"
  ],
  "author": "mg",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitepress": "1.0.0-rc.24",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "commitizen": "^4.3.0",
    "console-gui-tools": "^3.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.10",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "iconv-lite": "^0.6.3",
    "lint-staged": "^15.0.2",
    "lodash": "^4.17.21",
    "neofetch": "^7.1.0",
    "ping": "^0.4.4",
    "ps-list": "^8.1.1",
    "tree-cli": "^0.6.7",
    "tsx": "^3.14.0",
    "unbuild": "^2.0.0"
  },
  "files": [
    "./src/bin",
    "./dist",
    "README.md"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyong008/call-cli.git"
  }
}
